@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Main Menu</title>
    <style>
        body {
            margin: 0;
            padding: 0;
            background-image: url('https://videos.openai.com/vg-assets/assets%2Ftask_01jwa1wfj6fwhtgsgtxh0vvf5r%2F1748389239_img_1.webp?st=2025-05-27T21%3A49%3A21Z&se=2025-06-02T22%3A49%3A21Z&sks=b&skt=2025-05-27T21%3A49%3A21Z&ske=2025-06-02T22%3A49%3A21Z&sktid=a48cca56-e6da-484e-a814-9c849652bcb3&skoid=aa5ddad1-c91a-4f0a-9aca-e20682cc8969&skv=2019-02-02&sv=2018-11-09&sr=b&sp=r&spr=https%2Chttp&sig=KmU%2BXwpl9pE1Wic9pwIA3qm9KhSxpDFML%2BtKEMm4Kw8%3D&az=oaivgprodscus');
            background-size: cover;
            background-position: center;
            font-family: Arial, sans-serif;
            color: white;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            height: 100vh;
        }

        h1 {
            font-size: 4rem;
            background-color: rgba(0, 0, 0, 0.5);
            padding: 20px 40px;
            border-radius: 12px;
            margin-bottom: 40px;
        }

        .menu-button {
            font-size: 1.5rem;
            padding: 15px 30px;
            border: none;
            border-radius: 8px;
            background-color: rgba(255, 255, 255, 0.8);
            color: #000;
            cursor: pointer;
            transition: background-color 0.3s ease;
            text-decoration: none;
        }

            .menu-button:hover {
                background-color: rgba(255, 255, 255, 1);
            }
    </style>
</head>
<body>
    <h1>Паркинг</h1>
    <a class="menu-button" href="/Home/Index">Влез</a>
</body>
</html>
